{
    "name": "hiradumi",
    "version": "2.0.0",
    "description": "アスペクト比を維持して幅ぴったりにレイアウトするReactコンポーネント",
    "main": "lib/Hiradumi.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CALIL/hiradumi.git"
    },
    "author": "CALIL Inc.",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/CALIL/hiradumi/issues"
    },
    "homepage": "https://github.com/CALIL/hiradumi#readme",
    "scripts": {
        "start": "vite --open",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "compile": "gulp",
        "prepublish": "tsc",
        "//compile": "npm run compile:tsc && npm run compile:Hiradumi && npm run compile:DefaultItem",
        "compile:tsc": "tsc --project ./ --outDir ./tmp/",
        "compile:Hiradumi": "browserify -p esmify ./tmp/Hiradumi.js -o ./lib/Hiradumi.js --transform babelify",
        "compile:DefaultItem": "browserify -p esmify ./tmp/DefaultItem.js -o ./lib/DefaultItem.js --transform babelify"
    },
    "dependencies": {
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "core-js": "^3.10.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-virtualized-auto-sizer": "^1.0.5",
        "react-window": "^1.8.6",
        "whatwg-fetch": "^3.6.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.13.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "esmify": "^2.1.1",
        "gulp": "^4.0.2",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "pug": "^3.0.2",
        "realpathify": "^1.0.1",
        "sass": "^1.35.1",
        "tsify": "^5.0.4",
        "typescript": "^4.3.2",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vite": "^2.3.7",
        "vite-plugin-pug": "^0.3.0"
    }
}
